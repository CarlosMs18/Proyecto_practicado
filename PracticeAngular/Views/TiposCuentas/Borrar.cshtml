@model TipoCuenta

@{
    ViewData["Title"] = "Eliminar Cuenta";
}

<h1 class="display-4">Eliminar Cuenta</h1>

<h4>¿Esta seguro de eliminar el registro @Model.Nombre ?</h4>
<form asp-action="BorrarTipoCuenta">
    <input type="hidden" asp-for="Id" />
    <button class="btn btn-danger" >Eliminar</button>
    <button class="btn btn-secondary" asp-action="Index">Cancelar</button>
</form>
